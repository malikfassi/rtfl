{
  "configurations": [
    {
      "args": [
        "--config",
        "jest.vscode.config.js",
        "--runInBand",
        "--no-cache"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "test"
      },
      "internalConsoleOptions": "neverOpen",
      "name": "Debug Jest Tests",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "request": "launch",
      "type": "node"
    },
    {
      "args": [
        "--config",
        "jest.vscode.config.js",
        "--runInBand",
        "--no-cache",
        "${relativeFile}"
      ],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "test"
      },
      "internalConsoleOptions": "neverOpen",
      "name": "Debug Current Test File",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "request": "launch",
      "type": "node"
    }
  ],
  "version": "0.2.0"
}
