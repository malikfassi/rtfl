    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "4.0.0-beta1",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "npm-check-updates": "^17.1.14",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "tailwindcss": "^3.4.1",
