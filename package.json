  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "name": "rtfl",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "private": true,
  "scripts": {
    "build": "next build",
    "clean:db": "rm -f prisma/test-*.db prisma/test-*.db-journal && rm -rf prisma/test-dbs",
    "dev": "next dev",
    "full": "pnpm run generate-fixtures && pnpm eslint src && pnpm type-check && pnpm build && pnpm jest",
    "generate-fixtures": "tsx src/app/api/lib/test/fixtures/generator.ts",
    "generate-fixtures:watch": "nodemon --watch src/app/api/lib/test/fixtures/generator.ts --watch src/app/api/lib/test/fixtures/spotify_ids.ts --ext ts --exec 'pnpm generate-fixtures'",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:studio": "prisma studio",
    "start": "next start",
    "test": "jest",
    "test:clean": "npm run clean:db && jest",
    "test:failed": "jest --verbose --silent --passWithNoTests",
    "test:failed:config": "jest --config jest.failed.config.cjs",
    "test:failed:config:log": "jest --config jest.failed.config.cjs > test-results/failed-tests-detailed.log 2>&1",
    "test:failed:log": "jest --verbose --silent --passWithNoTests > test-results/failed-tests.log 2>&1",
    "test:failed:only": "jest --verbose --silent --passWithNoTests --onlyFailures",
    "test:failed:script": "./scripts/test-failed.sh",
    "test:failed:script:log": "./scripts/test-failed.sh log",
    "test:front": "jest --config jest.config.cjs",
    "test:integration": "jest --selectProjects integration",
    "test:integration:sequential": "jest --selectProjects integration --runInBand",
    "test:parallel": "npm run clean:db && jest --maxWorkers=50%",
    "test:sequential": "npm run clean:db && jest --runInBand",
    "test:unit": "jest --selectProjects unit",
    "test:watch": "jest --watch",
    "type-check": "rm -rf .next && tsc --noEmit"
  },
  "type": "module",
  "version": "0.1.0"
}
